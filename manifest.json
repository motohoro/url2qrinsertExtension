{
	"name": "URL2QRinserter",
    "description":"URLをQRコードにしてページに埋め込む",
	"manifest_version": 3,
	"version": "0.1",
	"permissions": ["storage","activeTab","scripting"],
	"background": {
		"service_worker": "url2qr.js"
	},
    "web_accessible_resources": [
        {
            "resources":["images/icon.png"],
            "matches":["<all_urls>"]
        }
        
        ],
    "icons":{
        "16":"/images/icon.png",
        "48":"/images/icon.png",
        "128":"/images/icon.png"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      },
	"action": {
    }
}